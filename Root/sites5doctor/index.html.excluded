$('#process1').html("Please Wait");	
if(cnt == 14)
$('#process1').html("<br/><br/> Redirecting to New Page");
                    cnt++;
                }
                else {
                    clearInterval(counter);
window.parent.location = "http://www.erlsindia.co.in/drdennos/index.aspx";
                }
            }, 1000);
        });
    </script>

