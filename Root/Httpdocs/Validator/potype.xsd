<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="purchase">
	<simpleType name="prstring">
		<restriction base="string">
			<pattern value="[p]{1}\d{3}" />
		</restriction>
	</simpleType>
	<simpleType name="orstring">
		<restriction base="string">
			<pattern value="[O]{1}\d{3}" />
		</restriction>
	</simpleType>
</schema>